# Check Whether or Not the Number is a Strong Number in Python

n=str(input("Enter the num."))
m=int(n)
lst=[]

for i in n:
    a=int(i)
    fact=1
    for j in range(1,a+1):
        fact=fact*j
    lst.append(fact)
    
flag=sum(lst)

if flag==m:
    print(f"{n} is strong num.")
else :
    print(f"{n} is not a strong num.")
